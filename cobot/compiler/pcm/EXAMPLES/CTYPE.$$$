#nolist E    9N
#ifndef islower E   10N
 E   11N
#define islower(x)  isamoung(x,"abcdefghijklmnopqrstuvwxyz") E   12N
#define isupper(x)  isamoung(x,"ABCDEFGHIJKLMNOPQRSTUVWXYZ") E   13N
#define isalnum(x)  isamoung(x,"0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz") E   14N
#define isalpha(x)  isamoung(x,"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz") E   15N
#define isdigit(x)  isamoung(x,"0123456789") E   16N
#define isspace(x)  (x==' ') E   17N
#define isxdigit(x) isamoung(x,"0123456789ABCDEFabcdef") E   18N
 E   19N
#endif E   20N
 E   21N
#list E   22 
